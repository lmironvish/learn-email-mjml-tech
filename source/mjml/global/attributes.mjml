<mj-attributes>

  <!-- global attr mjml -->

  <mj-all                                    font-family="Helvetica, Arial, sans-serif"
                                             font-size="20px"
                                             line-height="25px"
                                             font-weight="400"
                                             font-style="normal"
                                             color="#000000" />

  <mj-class name="h1"                        font-size="34px"
                                             line-height="40px"
                                             font-weight="700" />

  <mj-class name="h2"                        font-size="28px"
                                             line-height="35px"
                                             font-weight="700" />

  <mj-class name="h3"                        font-size="16px"
                                             line-height="22px"
                                             font-weight="700" />

  <mj-class name="small"                     font-size="14px"
                                             line-height="20px"
                                             color="#838383" />

  <mj-wrapper                                padding="0px"
                                             full-width="full-width"/>

  <mj-section                                padding="0px" />

  <mj-image                                  padding="0px"
                                             rel="noopener noreferrer" />

  <mj-button                                 padding="0px"
                                             rel="noopener noreferrer"
                                             target="_blank" />

  <mj-text                                   align="center"/>

  <mj-class name="divider"                   padding="0px"
                                             padding-bottom="20px"
                                             border-width="1px"
                                             border-style="solid"
                                             border-color="#838383" />

  <mj-class name="divider-spacer"            padding="10px 0px"
                                             border-width="0px"
                                             border-style="solid"
                                             border-color="#ffffff" />

  <mj-class name="container"                 width="87%" />

  <!-- helpers -->

  <!-- padding-bottom -->

  <mj-class name="pb0"                       padding-bottom="0px" />
  <mj-class name="pb10"                      padding-bottom="10px" />
  <mj-class name="pb15"                      padding-bottom="15px" />
  <mj-class name="pb20"                      padding-bottom="20px" />
  <mj-class name="pb25"                      padding-bottom="25px" />
  <mj-class name="pb30"                      padding-bottom="30px" />
  <mj-class name="pb40"                      padding-bottom="40px" />
  <mj-class name="pb50"                      padding-bottom="50px" />
  <mj-class name="pb60"                      padding-bottom="60px" />

  <!-- padding-top -->

  <mj-class name="pt0"                       padding-top="0px" />
  <mj-class name="pt10"                      padding-top="10px" />
  <mj-class name="pt15"                      padding-top="15px" />
  <mj-class name="pt20"                      padding-top="20px" />
  <mj-class name="pt25"                      padding-top="25px" />
  <mj-class name="pt30"                      padding-top="30px" />
  <mj-class name="pt40"                      padding-top="40px" />
  <mj-class name="pt50"                      padding-top="50px" />
  <mj-class name="pt60"                      padding-top="60px" />

  <!-- blocks -->

  <!-- title -->

  <mj-class name="views-title__img"                  width="128px"
                                                     padding="10px 20px"/>

  <!-- header -->

  <mj-class name="views-header__text"                font-family="Roboto, Tahoma, Veranda, sans-serif"
                                                     font-size="10px"
                                                     font-weight="700"
                                                     letter-spacing="2px"
                                                     line-height="16px"
                                                     text-align="center"
                                                     text-transform="uppercase"
                                                     color="#ffffff"
                                                     padding="24px 20px"
                                                     padding-bottom="0px"/>


  <mj-class name="views-header__caption"             font-family="Oswald, Arial, Helvetica, sans-serif";
                                                     font-size="64px"
                                                     font-style="italic"
                                                     font-weight="500"
                                                     line-height="64px"
                                                     text-align="center"
                                                     text-transform="uppercase"
                                                     padding="0px 20px"
                                                     color="#ffffff"/>

  <mj-class name="views-header__btn"                 font-family="Arial, Helvetica, sans-serif";
                                                     font-size="15px"
                                                     font-weight="bold"
                                                     line-height="22px"
                                                     text-align="center"
                                                     text-transform="uppercase"
                                                     padding="10px 20px"
                                                     color="#fde4ac"
                                                     background-color="#000000"/>

  <mj-class name="views-header__img"                 width="560px"
                                                     padding="10px 20px"
                                                     fluid-on-mobile="true"/>

  <!-- np-block -->

  <mj-class name="wrapper-divider-first"             background-url="https://i.postimg.cc/Y9pjq2vD/img-tech-bg-1.jpg"
                                                     background-position="center center"
                                                     background-repeat="no-repeat"
                                                     background-size="cover"
                                                     padding-top="90px"/>

  <mj-class name="wrapper-divider-second"            background-url="https://i.postimg.cc/hvvjTCb9/img-tech-bg-3.jpg"
                                                     background-position="center center"
                                                     background-repeat="no-repeat"
                                                     background-size="cover"
                                                     padding-top="90px"/>

  <mj-class name="wrapper-background-first"          background-url="https://i.postimg.cc/y8cNm39r/img-tech-bg-5.jpg"
                                                     background-position="center center"
                                                     background-repeat="no-repeat"
                                                     background-size="cover" />

  <mj-class name="views-new-product"                 background-color="#fde4ac"
                                                     padding="10px 0px"/>

  <mj-class name="views-new-product__btn"            font-family="Arial, Helvetica, sans-serif";
                                                     font-size="15px"
                                                     font-weight="bold"
                                                     line-height="22px"
                                                     text-align="center"
                                                     text-transform="uppercase"
                                                     color="#ffffff"
                                                     background-color="#000000"
                                                     padding="10px 20px"
                                                     align="left"
                                                     border-radius="3px"/>

  <mj-class name="views-new-product__text"           font-family="Helvetica, Arial, sans-serif"
                                                     font-size="14px"
                                                     font-style="normal"
                                                     line-height="22px"
                                                     font-weight="400"
                                                     text-align="left"
                                                     align="left"
                                                     padding="10px 20px"
                                                     color="#9d8f69" />

  <mj-class name="views-new-product__caption"        font-family="Arial, sans-serif"
                                                     font-size="36px"
                                                     font-weight="bold"
                                                     line-height="46px"
                                                     text-align="left"
                                                     padding="10px 20px"
                                                     align="left"
                                                     color="#000000"/>


  <mj-class name="views-new-product__img"            width="320px"
                                                     padding="10px 20px"
                                                     fluid-on-mobile="true"/>


  <!-- three-col -->


  <mj-class name="views-three-col__column"           padding-bottom="30px"/>

  <mj-class name="views-three-col__caption"          font-family="Arial, Helvetica, sans-serif";
                                                     font-size="16px"
                                                     font-style="italic"
                                                     font-weight="bold"
                                                     line-height="24px"
                                                     text-align="center"
                                                     text-transform="uppercase"
                                                     color="#ffffff"
                                                     padding="10px 20px"/>

  <mj-class name="views-three-col__text"             font-family="Helvetica, Arial, sans-serif"
                                                     font-size="14px"
                                                     font-style="normal"
                                                     font-weight="400"
                                                     line-height="22px"
                                                     text-align="center"
                                                     color="#9a9a9a"
                                                     padding="10px 20px"/>

  <mj-class name="views-three-col__btn"              font-size="15px"
                                                     line-height="22px"
                                                     font-weight="bold"
                                                     text-align="center"
                                                     color="#000000"
                                                     background-color="#fde4ac"
                                                     text-transform="uppercase"
                                                     padding="10px 20px"/>

  <mj-class name="views-three-col__img-cam"          padding="10px 20px"
                                                     fluid-on-mobile="true"/>

  <mj-class name="views-three-col__img-tel"          padding="10px 20px"
                                                     fluid-on-mobile="true"/>

  <mj-class name="views-three-col__img-hf"           padding="10px 20px"
                                                     fluid-on-mobile="true"/>
  <!-- vr -->

  <mj-class name="wrapper-background-second"         background-url="https://i.postimg.cc/HnNL2QBF/img-tech-bg-2.jpg"
                                                     background-position="center center"
                                                     background-repeat="no-repeat"
                                                     background-size="cover" />

  <mj-class name="views-vr"                          background-color="#fbf5e4"/>

  <mj-class name="views-vr__right-col"               vertical-align="bottom"/>

  <mj-class name="views-vr__btn"                     font-family="Arial, Helvetica, sans-serif";
                                                     font-size="15px"
                                                     font-weight="bold"
                                                     line-height="22px"
                                                     text-align="center"
                                                     align="left"
                                                     text-transform="uppercase"
                                                     color="#000000"
                                                     background-color="#fbf5e4"
                                                     padding="10px 20px"
                                                     width="175px"/>

  <mj-class name="views-vr__text"                    font-family="Helvetica, Arial, sans-serif"
                                                     font-size="14px"
                                                     font-style="normal"
                                                     line-height="22px"
                                                     font-weight="400"
                                                     text-align="left"
                                                     align="left"
                                                     padding="10px 20px"
                                                     padding="0px"
                                                     width="200px"
                                                     color="#555555"/>

  <mj-class name="views-vr__caption"                 font-family="Arial, sans-serif"
                                                     font-size="36px"
                                                     font-weight="bold"
                                                     line-height="46px"
                                                     text-align="left"
                                                     align="left"
                                                     padding="10px 20px"
                                                     width="200px"
                                                     color="#000000"/>


  <mj-class name="views-vr__img"                     padding-top="10px"
                                                     fluid-on-mobile="true"/>


  <!-- footer -->

  <mj-class name="views-footer__column"              width="300px"/>

  <mj-class name="views-footer__social"              icon-size="32px "/>

  <mj-class name="views-footer__copyright"           font-family="Arial, sans-serif"
                                                     font-size="14px"
                                                     line-height="22px"
                                                     text-align="center"
                                                     color="#ffffff"
                                                     padding="20px"/>




</mj-attributes>
